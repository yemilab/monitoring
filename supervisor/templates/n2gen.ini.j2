[program:n2gen_{{ n2gen['tag'] }}]
command={{ slowmonitoring['basedir'] }}/venv/bin/python scripts/n2gen.py
process_name=%(program_name)s
directory={{ slowmonitoring['basedir'] }}
autostart=true
autorestart=unexpected
user=root
redirect_stderr=true
stdout_logfile={{ slowmonitoring['basedir'] }}/log/n2gen_{{ n2gen['tag'] }}.log
environment = DEVICE_IPADDR="{{ n2gen['ip'] }}",DEVICE_PORT="{{ n2gen['port'] }}",DEVICE_TAG="{{ n2gen['tag'] }}"
