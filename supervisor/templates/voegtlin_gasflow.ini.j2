[program:voegtlin_gasflow_{{ gasflow['tag'] }}]
command={{ slowmonitoring['basedir'] }}/venv/bin/python scripts/voegtlin_gasflow.py
process_name=%(program_name)s
directory={{ slowmonitoring['basedir'] }}
autostart=true
autorestart=unexpected
user=root
redirect_stderr=true
stdout_logfile={{ slowmonitoring['basedir'] }}/log/voegtlin_gasflow_{{ gasflow['tag'] }}.log
environment = DEVICE="{{ gasflow['dev'] }}",DEVICE_TAG="{{ gasflow['tag'] }}",DEVICE_TYPE="{{ gasflow['type'] }}"
