[program:apcups_{{ apcups['tag'] }}]
command={{ slowmonitoring['basedir'] }}/venv/bin/python scripts/apc_ups.py
process_name=%(program_name)s
directory={{ slowmonitoring['basedir'] }}
autostart=true
autorestart=unexpected
user=root
redirect_stderr=true
stdout_logfile={{ slowmonitoring['basedir'] }}/log/apcups_{{ apcups['tag'] }}.log
environment = DEVICE_IPADDR="{{ apcups['ip'] }}",DEVICE_PORT="{{ apcups['port'] }}",DEVICE_TAG="{{ apcups['tag'] }}"
