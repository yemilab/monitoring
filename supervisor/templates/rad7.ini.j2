[program:rad7-telnet_{{ rad7['tag'] }}]
command={{ slowmonitoring['basedir'] }}/venv/bin/python scripts/rad7-telnet.py
process_name=%(program_name)s
directory={{ slowmonitoring['basedir'] }}
autostart=true
autorestart=unexpected
user=root
redirect_stderr=true
stdout_logfile={{ slowmonitoring['basedir'] }}/log/rad7-telnet_{{ rad7['tag'] }}.log
environment = DEVICE_IPADDR="{{ rad7['ip'] }}",DEVICE_PORT="{{ rad7['port'] }}",DEVICE_TAG="{{ rad7['tag'] }}",DEVICE_SN="{{ rad7['sn'] }}"
